{
  "name": "@sap/eslint-plugin-ui5-jsdocs",
  "version": "2.0.5",
  "description": "verifies that sap.ui.define module paths described by JSDoc annotations",
  "license": "See LICENSE.txt",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "nyc mocha tests --recursive",
    "lint": "eslint",
    "format:fix": "prettier --write \"**/*.@(ts|js|json|md|yml)\""
  },
  "dependencies": {
    "comment-parser": "^0.5.4",
    "eslint": "^6.0.1",
    "lodash": "4.17.21",
    "requireindex": "^1.2.0",
    "string-format": "^2.0.0"
  },
  "devDependencies": {
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-standard": "^4.0.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "mocha": "^6.1.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 97,
    "statements": 97,
    "functions": 100,
    "branches": 100,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "tests/**"
    ],
    "reporter": [
      "lcov",
      "cobertura",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "temp-dir": "./target/coverage-tmp",
    "report-dir": "./target/coverage"
  }
}
